{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "cloudConfig": {
            "type": "object",
            "properties": {
                "global": {
                    "type": "object",
                    "properties": {
                        "auth-url": {
                            "type": "string"
                        },
                        "domain-name": {
                            "type": "string"
                        },
                        "password": {
                            "type": "string"
                        },
                        "region": {
                            "type": "string"
                        },
                        "tenant-id": {
                            "type": "string"
                        },
                        "username": {
                            "type": "string"
                        }
                    }
                },
                "loadBalancer": {
                    "type": "object",
                    "properties": {
                        "create-monitor": {
                            "type": "boolean"
                        },
                        "floating-network-id": {
                            "type": "string"
                        },
                        "internal-lb": {
                            "type": "boolean"
                        }
                    }
                },
                "networking": {
                    "type": "object",
                    "properties": {
                        "ipv6-support-disabled": {
                            "type": "boolean"
                        },
                        "public-network-name": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "name": {
            "type": "string"
        },
        "openstack-cinder-csi": {
            "type": "object",
            "properties": {
                "clusterID": {
                    "type": "object"
                },
                "csi": {
                    "type": "object",
                    "properties": {
                        "plugin": {
                            "type": "object",
                            "properties": {
                                "image": {
                                    "type": "object",
                                    "properties": {
                                        "pullPolicy": {
                                            "type": "string"
                                        },
                                        "repository": {
                                            "type": "string"
                                        },
                                        "tag": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "volumeMounts": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "mountPath": {
                                                "type": "string"
                                            },
                                            "name": {
                                                "type": "string"
                                            },
                                            "readOnly": {
                                                "type": "boolean"
                                            }
                                        }
                                    }
                                },
                                "volumes": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "hostPath": {
                                                "type": "object",
                                                "properties": {
                                                    "path": {
                                                        "type": "string"
                                                    }
                                                }
                                            },
                                            "name": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        },
                        "snapshotController": {
                            "type": "object",
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                }
                            }
                        }
                    }
                },
                "secret": {
                    "type": "object",
                    "properties": {
                        "create": {
                            "type": "boolean"
                        },
                        "enabled": {
                            "type": "boolean"
                        },
                        "name": {
                            "type": "string"
                        }
                    }
                },
                "storageClass": {
                    "type": "object",
                    "properties": {
                        "delete": {
                            "type": "object",
                            "properties": {
                                "allowVolumeExpansion": {
                                    "type": "boolean"
                                },
                                "isDefault": {
                                    "type": "boolean"
                                }
                            }
                        },
                        "enabled": {
                            "type": "boolean"
                        },
                        "retain": {
                            "type": "object",
                            "properties": {
                                "allowVolumeExpansion": {
                                    "type": "boolean"
                                },
                                "isDefault": {
                                    "type": "boolean"
                                }
                            }
                        }
                    }
                }
            }
        },
        "openstack-cloud-controller-manager": {
            "type": "object",
            "properties": {
                "controllerExtraArgs": {
                    "type": "object"
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "repository": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                },
                "nodeSelector": {
                    "type": "object",
                    "properties": {
                        "node-role.kubernetes.io/control-plane": {
                            "type": "string"
                        }
                    }
                },
                "secret": {
                    "type": "object",
                    "properties": {
                        "create": {
                            "type": "boolean"
                        }
                    }
                },
                "tolerations": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "effect": {
                                "type": "string"
                            },
                            "key": {
                                "type": "string"
                            },
                            "value": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "project": {
            "type": "object",
            "properties": {
                "branch": {
                    "type": "string"
                },
                "commit": {
                    "type": "string"
                }
            }
        },
        "secret": {
            "type": "object",
            "properties": {
                "create": {
                    "type": "boolean"
                }
            }
        },
        "serviceType": {
            "type": "string"
        }
    }
}
